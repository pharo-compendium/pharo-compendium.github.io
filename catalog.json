---
layout: null
---
[
    {% for entry in site.posts %}
        {
            "title": "{{ entry.title }}",              
            "category": "{{ entry.category }}",                                      
            "homepage": "{{ entry.homepage }}",
            "url": "{{ site.BASE_PATH }}{{ entry.url }}",
            "icon": "{{ entry.icon }}",
            "expression": "{{ entry.expression }}"                
        }
        {% unless forloop.last %},{% endunless %}
    {% endfor %}
]